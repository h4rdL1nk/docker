pipeline {
    agent any
    environment{
        HTTP_PROXY = 'http://proxy2pdi.service.dsn.inet:6666'
        HTTPS_PROXY = 'http://proxy2pdi.service.dsn.inet:6666'
    }
    options {
        timestamps()
        disableConcurrentBuilds()
    }
    stages {
        stage('Build container'){
            steps{
                sh script: """
                    set +x
                    cd jenkins
                    docker build -t test-alpine -f Dockerfile.alpine .
                """
            }
        }
    }
}